<?xml version="1.0"?> 

<!-- Generic protocol to send joystick axis over network -->

<PropertyList>
<generic>

   <input>
     <line_separator>newline</line_separator>
     <var_separator>|</var_separator>
     <binary_mode>false</binary_mode>

     <chunk>
       <name>Aileron</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/flight/aileron</node>
     </chunk>

     <chunk>
       <name>Elevator</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/flight/elevator</node>
     </chunk>

	 <chunk>
       <name>Throttle</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/engines/engine[0]/throttle</node>
     </chunk>

 	 <chunk>
       <name>Throttle</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/engines/engine[1]/throttle</node>
     </chunk>

	 <chunk>
       <name>Throttle</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/engines/engine[2]/throttle</node>
     </chunk>

	 <chunk>
       <name>Throttle</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/engines/engine[3]/throttle</node>
     </chunk>

	 
     <chunk>
       <name>Rudder</name>
       <format>%f</format>
       <type>float</type>
       <node>/controls/flight/rudder</node>
     </chunk>

   </input>
</generic>
</PropertyList>

